<h3>List OF Aircrafts</h3>

<table class="table table-responsive table-striped table-bordered">
    <thead>
        <th>ID</th>
        <th>Aircraft No.</th>
        <th>Name</th>
        <th>Capacity</th>
        <th>Type</th>
        <th colspan="2">Action</th>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>{{ID}}</td>
            <td>{{aircraftno}}</td>
             <td>{{name}}</td>
             <td>{{capacity}}</td>
             <td>{{type}}</td>
             <td> <a class="btn btn-sm btn-warning" href="/manageaircrafts/update/{{ID}}">Edit</a></td>
             <td><a class="btn btn-sm btn-danger" href="/manageaircrafts/delete/{{ID}}">Delete</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>