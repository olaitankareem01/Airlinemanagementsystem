<h3>List OF Passengers</h3>
<table class="table table-responsive table-striped table-bordered">
<thead>
       <th>ID</th>
       <th>FIRST NAME</th>
        <th>LAST NAME</th>
         <th>EMAIL</th>
        <th>PHONE NO</th>
         <th>AGE</th>
         <th>SEX</th>
        <th colspan="2">ACTION</th>
</thead>
{{#each data}}
 <tbody>
   <tr>
      <td>{{ID}}</td>
      <td>{{firstname}}</td>  
      <td>{{lastname}}</td> 
      <td>{{email}}</td> 
      <td> {{phone_no}}</td>
      <td> {{age}}</td>
      <td> {{sex}}</td>
      <td> <a class="btn btn-sm btn-warning" href="/managepassengers/update/{{ID}}">Edit</a></td>
       <td><a class="btn btn-sm btn-danger" href="/managepassengers/delete/{{ID}}">Delete</a></td>
      </tr>
   {{/each}}
</tbody>

</table>
